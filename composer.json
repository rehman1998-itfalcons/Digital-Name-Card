{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "version": "6.6.0",
    "require": {
        "php": "^7.4|^8.0",
        "doctrine/dbal": "^3.3",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "gerardojbaez/money": "^0.3.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "infyomlabs/laravel-generator": "dev-infyom-dev",
        "jenssegers/agent": "^2.6",
        "jeroendesloovere/vcard": "^1.7",
        "lab404/laravel-impersonate": "^1.7",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^8.40",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.2",
        "league/flysystem-aws-s3-v3": "~1",
        "livewire/livewire": "^2.10",
        "mariuzzo/laravel-js-localization": "^1.8",
        "mews/purifier": "^3.3",
        "paypal/paypal-checkout-sdk": "^1.0",
        "rap2hpoutre/laravel-log-viewer": "^2.0",
        "rappasoft/laravel-livewire-tables": "^1.24",
        "razorpay/razorpay": "^2.8",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-cookie-consent": "^2.12",
        "spatie/laravel-medialibrary": "^9.0.0",
        "spatie/laravel-permission": "^4.2",
        "spatie/laravel-sitemap": "5.9.2",
        "srmklive/paypal": "^3.0",
        "stancl/tenancy": "^3.4",
        "stevebauman/location": "^6.3",
        "stidges/laravel-country-flags": "^2.0",
        "stripe/stripe-php": "^9.0.0",
        "tightenco/ziggy": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.10",
        "beyondcode/laravel-query-detector": "^1.5",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.3",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
